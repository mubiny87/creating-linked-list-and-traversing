#include<iostream>
using namespace std;

struct Node
{
    int data;
    Node *next;
};

void printl(Node *head)
    {
        Node  *temp=head;
        while(temp!=NULL)
        {
            cout<<temp->data<<" ";
            temp=temp->next;
        }
        cout<< endl;
    }


int main()
{
    Node *head=new Node();
    Node *third=new Node();
    Node *second=new Node();

    head->data=10;
    second->data=20;
    third->data=30;

    head->next=second;
    second->next=third;
    third->next=NULL;


    printl(head);


    return 0;

}
